@model IEnumerable<TestApp.Models.LikedSongs>

@{
    ViewBag.Title = "LikedSongs";
}

<h2>LikedSongs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Music.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.MyUserInfo.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Music.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.MyUserInfo.Id)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
